<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/purchaseOverviewFragment">
    <fragment
        android:id="@+id/purchaseOverviewFragment"
        android:name="com.example.modueleccar.ui.HomeFragment"
        android:label="fragment_purchase_overview"
        tools:layout="@layout/fragment_home">
        <argument
            android:name="chargerDetail"
            app:argType="com.example.modueleccar.data.ChargerDetail" />
        <action
            android:id="@+id/action_purchaseOverviewFragment_to_stationDetailInfoFragment"
            app:destination="@id/stationDetailInfoFragment" />
        <action
            android:id="@+id/action_purchaseOverviewFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_purchaseOverviewFragment_to_acceptRequestFragment"
            app:destination="@id/acceptRequestFragment" />
        <action
            android:id="@+id/action_purchaseOverviewFragment_to_receiveRequestFragment"
            app:destination="@id/receiveRequestFragment" />
    </fragment>
    <fragment
        android:id="@+id/chooseTimeFragment"
        android:name="com.example.modueleccar.ui.purchase.ChooseTimeFragment"
        android:label="fragment_choose_time"
        tools:layout="@layout/fragment_choose_time">
        <action
            android:id="@+id/action_chooseTimeFragment_to_sendRequestFragment"
            app:destination="@id/sendRequestFragment" />
    </fragment>
    <fragment
        android:id="@+id/receiveRequestFragment"
        android:name="com.example.modueleccar.ui.purchase.ReceiveRequestFragment"
        android:label="fragment_receive_request"
        tools:layout="@layout/fragment_receive_request">
        <action
            android:id="@+id/action_receiveRequestFragment_to_paymentFragment"
            app:destination="@id/paymentFragment" />
    </fragment>
    <fragment
        android:id="@+id/sendRequestFragment"
        android:name="com.example.modueleccar.ui.purchase.SendRequestFragment"
        android:label="fragment_send_request"
        tools:layout="@layout/fragment_send_request">
        <action
            android:id="@+id/action_sendRequestFragment_to_receiveRequestFragment"
            app:destination="@id/receiveRequestFragment" />
    </fragment>
    <fragment
        android:id="@+id/stationDetailInfoFragment"
        android:name="com.example.modueleccar.ui.purchase.StationDetailInfoFragment"
        android:label="fragment_station_detail_info"
        tools:layout="@layout/fragment_station_detail_info">
        <argument android:name="chargerDetail" />
        <action
            android:id="@+id/action_stationDetailInfoFragment_to_chooseTimeFragment"
            app:destination="@id/chooseTimeFragment" />
    </fragment>
    <fragment
        android:id="@+id/paymentFragment"
        android:name="com.example.modueleccar.ui.purchase.PaymentFragment"
        android:label="fragment_payment"
        tools:layout="@layout/fragment_payment">
        <action
            android:id="@+id/action_paymentFragment_to_completePaymentFragment"
            app:destination="@id/completePaymentFragment" />
    </fragment>
    <fragment
        android:id="@+id/completePaymentFragment"
        android:name="com.example.modueleccar.ui.purchase.CompletePaymentFragment"
        android:label="CompletePaymentFragment">
        <action
            android:id="@+id/action_completePaymentFragment_to_useDescriptionFragment"
            app:destination="@id/useDescriptionFragment" />
    </fragment>
    <fragment
        android:id="@+id/useDescriptionFragment"
        android:name="com.example.modueleccar.ui.purchase.UseDescriptionFragment"
        android:label="fragment_use_description"
        tools:layout="@layout/fragment_use_description" />
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.modueleccar.ui.sell.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register">
        <action
            android:id="@+id/action_registerFragment_to_conditionCheckFragment"
            app:destination="@id/conditionCheckFragment" />
    </fragment>
    <fragment
        android:id="@+id/conditionCheckFragment"
        android:name="com.example.modueleccar.ui.sell.ConditionCheckFragment"
        android:label="fragment_condition_check"
        tools:layout="@layout/fragment_condition_check">
        <action
            android:id="@+id/action_conditionCheckFragment_to_registerLocationFragment"
            app:destination="@id/registerLocationFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerLocationFragment"
        android:name="com.example.modueleccar.ui.sell.RegisterLocationFragment"
        android:label="fragment_register_location"
        tools:layout="@layout/fragment_register_location" >
        <action
            android:id="@+id/action_registerLocationFragment_to_registerDetailFragment"
            app:destination="@id/registerDetailFragment" />
    </fragment>

    <fragment
        android:id="@+id/acceptRequestFragment"
        android:name="com.example.modueleccar.ui.purchase.AcceptRequestFragment"
        android:label="fragment_accept_request"

        tools:layout="@layout/fragment_accept_request">
        <action
            android:id="@+id/action_acceptRequestFragment_to_purchaseOverviewFragment"
            app:destination="@id/purchaseOverviewFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerDetailFragment"
        android:name="com.example.modueleccar.ui.sell.RegisterDetailFragment"
        android:label="fragment_register_detail"
        tools:layout="@layout/fragment_register_detail" />
</navigation>